#%Module1.0#####################################################################
##
## SCS Vivado module file
##
proc ModulesHelp { } {
	global rkoversion

	puts stderr "\tLoads Xilinx Vivado version 2015.2"
}

module-whatis	"Xilinx Vivado 2015.2"

set rkoversion 3.2.10

conflict vivado 

prepend-path PATH /opt/toolchain/xilinx/Vivado_2015.2/Vivado/2015.2/bin
setenv LM_LICENSE_FILE "2100@flexlm.scs-ad.scs.ch"
